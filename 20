#include<iostream> 
using namespace std; 
class base{ 
public: 
void print(){ 
cout<<"Print() of base class"<<endl; 
} 
}; 
class derived:public base{ 
public: 
void show(){ 
cout<<"show() of derived class"<<endl; 
} 
}; 
int main(){ 
derived d; 
derived *dptr; 
dptr=&d; 
dptr->show(); 
dptr->print(); 
} 
