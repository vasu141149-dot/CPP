#include<iostream> 
using namespace std; 
class Base 
{ 
public: 
void print() 
{ 
cout<<"print() of Base class"<<endl; 
} 
}; 
class Derived:public Base 
{ 
public: 
void show() 
{ 
cout<<"show() of Derived class"<<endl; 
} 
}; 
int main() 
{ 
Base b,*bptr; 
bptr=&b; 
bptr->print(); 
return 0; 
} 
