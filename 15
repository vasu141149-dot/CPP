#include<iostream> 
using namespace std; 
class person 
{ 
 protected: 
  string name; 
  char gender; 
  int age; 
  public: 
   void getp() 
   { 
    cout<<"enter name,age,gender"; 
    cin>>name>>age>>gender; 
   } 
   void showp() 
   { 
    cout<<"name="<<name<<endl; 
    cout<<"age="<<age<<endl; 
    cout<<"gender="<<gender<<endl; 
     
   } 
}; 
class student:public person 
{ 
 private: 
  int pinno; 
  float per; 
  public: 
   void getst() 
   { 
    getp(); 
    pinno=501; 
    per=98.9; 
   } 
   void display() 
   { 
    showp(); 
    cout<<"pinno="<<pinno<<endl; 
    cout<<"percentage="<<per; 
   } 
}; 
int main() 
{ 
 student s; 
 s.getst(); 
 s.display(); 
} 
